Host *
    SendEnv no

Host bitbucket.org
    Compression yes
    ControlMaster yes
    ControlPersist 1h

Host gh
    HostName github.com
    User git
    Compression yes
    ControlMaster yes
    ControlPersist 1h

Host koding
  User captn3m0
  HostName vm-0.captn3m0.koding.kd.io
  ProxyCommand ssh %r@ssh.koding.com nc %h %p
  Protocol 2
  ServerAliveInterval 60